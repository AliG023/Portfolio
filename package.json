{
   "name": "React-portfolio",
   "version": "1.0.0",
   "description": "A React portfolio website",
   "main": "index.js",
   "type": "module",
   "private": true,
   "workspaces": [
      "client",
      "server"
   ],
   "scripts": {
      "dev": "concurrently \"npm run dev --prefix client\" \"nodemon server/server.js\"",
      "build": "npm run build --prefix client",
      "start": "node server/server.js",
      "preview": "npm run preview --prefix client -- --host 0.0.0.0"
   },
   "dependencies": {
      "bcrypt": "^6.0.0",
      "body-parser": "^2.2.0",
      "compression": "^1.8.0",
      "concurrently": "^9.2.1",
      "cookie-parser": "^1.4.7",
      "cors": "^2.8.5",
      "crypto": "^1.0.1",
      "dotenv": "^17.2.3",
      "express": "^5.1.0",
      "express-jwt": "^8.5.1",
      "global": "^4.4.0",
      "helmet": "^8.1.0",
      "jsonwebtoken": "^9.0.2",
      "mongodb": "^6.20.0",
      "mongoose": "^8.19.1",
      "morgan": "^1.10.1",
      "react": "^19.1.1",
      "react-dom": "^19.1.1",
      "react-router-dom": "^7.8.2",
      "vite": "^7.1.11"
   },
   "devDependencies": {
      "@eslint/js": "^9.33.0",
      "@types/react": "^19.1.10",
      "@types/react-dom": "^19.1.7",
      "@vitejs/plugin-react": "^5.1.0",
      "eslint": "^9.33.0",
      "eslint-plugin-react-hooks": "^5.2.0",
      "eslint-plugin-react-refresh": "^0.4.20",
      "globals": "^16.3.0",
      "nodemon": "^3.1.10",
      "vite": "^7.1.2"
   }
}
